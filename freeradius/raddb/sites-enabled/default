# Default virtual server
server default {
    listen {
        type = auth
        ipaddr = *
        port = 0
    }

    listen {
        type = acct
        ipaddr = *
        port = 0
    }

    authorize {
        preprocess
        sql
        pap
    }

    authenticate {
        Auth-Type PAP {
            pap
        }
    }

    preacct {
        preprocess
        acct_unique
    }

    accounting {
        sql
    }

    session {
        sql
    }

    post-auth {
        sql
        Post-Auth-Type REJECT {
            sql
        }
    }
}
